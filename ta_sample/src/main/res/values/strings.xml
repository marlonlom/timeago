  <string name="tabbed_main_detail_how_to" formatted="false">
        <![CDATA[
        Input some date time value as milliseconds:
        <br><br>
        <i>long time = System.currentTimeMillis();</i>
        <br><br>
        Use the <b>TimeAgo</b> class:
        <br><br>
        <i>
            String timeAgo = TimeAgo.using(time<br> [, new TimeAgoMessages.Builder()<br> .defaultLocale().build()]);
        </i>
        <br><br>
        For specific language usage, use <b>TimeAgoMessages</b>.
        <br><br><i>Languages supported</i>: Spanish (es), English (en), German (de), French (fr), Italian (it), Portuguese (pt).
        ]]>
    </string>
  <string name="tabbed_main_detail_from" formatted="false"><![CDATA[%s<br>]]></string>
  <string name="tabbed_main_detail_until" formatted="false"><![CDATA[%s<br>]]></string>
